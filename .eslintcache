SE_STATE\n  //* ======================\n  const [blobData, setBlobData] = useState();\n  //* ======================\n  //*    USE_EFFECT\n  //* ======================\n  useEffect(() => {\n    if (blobData) {\n      const formData = new FormData();\n      formData.append('profile_img', blobData);\n      axios\n        .post('/api/users/upload', formData, {\n          header: { 'content-type': 'multipart/form-data' },\n        })\n        .then((response) => {\n          onChange('profileImage', response.data.image);\n        });\n    }\n  }, [blobData]);\n  //* ======================\n  //*    CONDITIONAL_VARIABLE\n  //* ======================\n  const conditionImage =\n    profileImage[0] === 'u'\n      ? `http://localhost:5000/${profileImage}`\n      : profileImage;\n  //* ======================\n  //*    RENDER\n  //* ======================\n  return (\n    <BlockDiv>\n      <PrevBtn>\n        <GrFormPrevious onClick={() => changePages('prev')} />\n      </PrevBtn>\n      <RegisterInputText>사진 추가</RegisterInputText>\n      <Strong>나의 가장 멋진 사진!</Strong>\n      <Preview src={conditionImage} alt=\"profileImage\" />\n      <ImageCrop setBlobData={setBlobData} />\n      <ContinueBtnDiv>\n        <ContinueBtn onClick={onSubmitHandler}>완료</ContinueBtn>\n      </ContinueBtnDiv>\n    </BlockDiv>\n  );\n};\nexport default InfoUserImage;\n","/Users/8stepsMAC/Downloads/Project/tindux-client/src/components/systems/landingPage/Landing.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/components/contents/lobby/Lobby.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/containers/LoginContainer.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/components/systems/user_info/data/locationList.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/components/utils/ImageCrop.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/components/systems/user_login/LoginModal.jsx",[],"/Users/8stepsMAC/Downloads/Project/tindux-client/src/compone